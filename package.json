{
  "name": "react-native-executorch-monorepo",
  "version": "0.0.0",
  "packageManager": "yarn@4.1.1",
  "workspaces": {
    "packages": [
      "packages/react-native-executorch",
      "apps/llm",
      "apps/computer-vision",
      "apps/speech-to-text",
      "apps/text-embeddings"
    ]
  },
  "scripts": {
    "lint": "yarn workspaces foreach --all --parallel run lint",
    "typecheck": "yarn workspaces foreach --all --parallel run typecheck"
  },
  "private": true,
  "devDependencies": {
    "@cspell/eslint-plugin": "^8.19.0",
    "@evilmartians/lefthook": "^1.5.0",
    "@react-native/eslint-config": "^0.79.0",
    "cspell": "^8.19.0",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.3.3",
    "prettier-plugin-jsdoc": "^1.3.0",
    "typescript": "~5.8.3"
  }
}
