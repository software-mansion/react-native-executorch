{
  "name": "react-native-executorch-monorepo",
  "version": "0.0.0",
  "packageManager": "yarn@4.1.1",
  "workspaces": {
    "packages": [
      "packages/react-native-executorch",
      "apps/llama",
      "apps/computer-vision",
      "apps/speech-to-text"
    ]
  },
  "private": true,
  "scripts": {
    "build": "yarn workspaces foreach --all --parallel --topological-dev run build"
  },
  "devDependencies": {
    "prettier": "^3.3.3",
    "prettier-plugin-jsdoc": "^1.3.0",
    "typescript": "~5.3.0"
  }
}
