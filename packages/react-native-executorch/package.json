{
  "name": "react-native-executorch",
  "version": "0.3.1",
  "description": "An easy way to run AI models in react native with ExecuTorch",
  "source": "./src/index.ts",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/module/index.js",
  "types": "./lib/typescript/index.d.ts",
  "react-native": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "build": "bob build",
    "lint": "eslint . --ext .ts,.tsx,.js"
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "executorch",
    "ai",
    "llm",
    "coreml",
    "xnnpack",
    "llama"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/software-mansion/react-native-executorch.git",
    "directory": "packages/react-native-executorch"
  },
  "author": "Norbert Klockiewicz <norbert.klockiewicz@swmansion.com> (https://github.com/NorbertKlockiewicz)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/software-mansion/react-native-executorch/issues"
  },
  "homepage": "https://docs.swmansion.com/react-native-executorch",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@react-native-community/cli": "latest",
    "@types/jest": "^29.5.5",
    "@types/react": "^18.2.44",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "prettier": "^3.0.3",
    "react": "18.3.1",
    "react-native": "0.76.9",
    "react-native-builder-bob": "^0.30.2",
    "typescript": "^5.2.2"
  },
  "resolutions": {
    "@types/react": "^18.2.44"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "module"
      ],
      [
        "typescript",
        {
          "project": "tsconfig.json"
        }
      ]
    ]
  },
  "codegenConfig": {
    "name": "RnExecutorchSpec",
    "type": "all",
    "jsSrcsDir": "./src/native",
    "android": {
      "javaPackageName": "com.swmansion.rnexecutorch"
    }
  },
  "dependencies": {
    "expo": "^52.0.37",
    "expo-asset": "~11.0.5",
    "expo-file-system": "~18.0.12",
    "react-native-live-audio-stream": "^1.1.1"
  }
}
